//Analog Parameter Setting//
(* Write *)
// RWwm0 -> Ch1 Average Setting
// RWwm1 -> Ch2 Average Setting
// RWwm2 -> Ch3 Average Setting
// RWwm3 -> Ch4 Average Setting
(* Read *)
// RWrn0 -> Ch1 Digital Output
// RWrn1 -> Ch2 Digital Output
// RWrn2 -> Ch3 Digital Output
// RWrn3 -> Ch4 Digital Output

//Box1 -> Start (Write) W140, (Read) W540 || End (Write) W15F, (Read) W55F

//H400 -> D0Z0

//VAR_GLOBAL
   // Analog_Parameter_Setting_Fn_CH2: BOOL; (* Comment *)
//END_VAR

IF Analog_Parameter_Setting_Fn_CH1 = H1 THEN
	EMOVP(TRUE,E10.00,i_Analog_MAX_CH1);
	EMOVP(TRUE,E-10.00,i_Analog_MIN_CH1);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH1);
	EMOVP(TRUE,E-16000.00,i_Digital_MIN_CH1);
	ELSIF Analog_Parameter_Setting_Fn_CH1 = H2 THEN
	EMOVP(TRUE,E5.00,i_Analog_MAX_CH1);
	EMOVP(TRUE,E0.00,i_Analog_MIN_CH1);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH1);
	EMOVP(TRUE,E0.00,i_Digital_MIN_CH1);
	ELSIF Analog_Parameter_Setting_Fn_CH1 = H3 THEN
	EMOVP(TRUE,E5.00,i_Analog_MAX_CH1);
	EMOVP(TRUE,E1.00,i_Analog_MIN_CH1);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH1);
	EMOVP(TRUE,E0.00,i_Digital_MIN_CH1);
	ELSIF Analog_Parameter_Setting_Fn_CH1 = H4 THEN
	EMOVP(TRUE,E20.00,i_Analog_MAX_CH1);
	EMOVP(TRUE,E0.00,i_Analog_MIN_CH1);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH1);
	EMOVP(TRUE,E0.00,i_Digital_MIN_CH1);
	ELSIF Analog_Parameter_Setting_Fn_CH1 = H5 THEN
	EMOVP(TRUE,E20.00,i_Analog_MAX_CH1);
	EMOVP(TRUE,E4.00,i_Analog_MIN_CH1);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH1;
	EMOVP(TRUE,E0.00,i_Digital_MIN_CH1);
END_IF;


IF Analog_Parameter_Setting_Fn_CH2 = H1 THEN
	EMOVP(TRUE,E10.00,i_Analog_MAX_CH2);
	EMOVP(TRUE,E-10.00,i_Analog_MIN_CH2);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH2);
	EMOVP(TRUE,E-16000.00,i_Digital_MIN_CH2);
	ELSIF Analog_Parameter_Setting_Fn_CH2 = H2 THEN
	EMOVP(TRUE,E5.00,i_Analog_MAX_CH2);
	EMOVP(TRUE,E0.00,i_Analog_MIN_CH2);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH2);
	EMOVP(TRUE,E0.00,i_Digital_MIN_CH2);
	ELSIF Analog_Parameter_Setting_Fn_CH2 = H3 THEN
	EMOVP(TRUE,E5.00,i_Analog_MAX_CH2);
	EMOVP(TRUE,E1.00,i_Analog_MIN_CH2);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH2);
	EMOVP(TRUE,E0.00,i_Digital_MIN_CH2);
	ELSIF Analog_Parameter_Setting_Fn_CH2 = H4 THEN
	EMOVP(TRUE,E20.00,i_Analog_MAX_CH2);
	EMOVP(TRUE,E0.00,i_Analog_MIN_CH2);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH2);
	EMOVP(TRUE,E0.00,i_Digital_MIN_CH2);
	ELSIF Analog_Parameter_Setting_Fn_CH2 = H5 THEN
	EMOVP(TRUE,E20.00,i_Analog_MAX_CH2);
	EMOVP(TRUE,E4.00,i_Analog_MIN_CH2);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH2;
	EMOVP(TRUE,E0.00,i_Digital_MIN_CH2);
END_IF;

IF Analog_Parameter_Setting_Fn_CH3 = H1 THEN
	EMOVP(TRUE,E10.00,i_Analog_MAX_CH3);
	EMOVP(TRUE,E-10.00,i_Analog_MIN_CH3);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH3);
	EMOVP(TRUE,E-16000.00,i_Digital_MIN_CH3);
	ELSIF Analog_Parameter_Setting_Fn_CH3 = H2 THEN
	EMOVP(TRUE,E5.00,i_Analog_MAX_CH3);
	EMOVP(TRUE,E0.00,i_Analog_MIN_CH3);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH3);
	EMOVP(TRUE,E0.00,i_Digital_MIN_CH3);
	ELSIF Analog_Parameter_Setting_Fn_CH3 = H3 THEN
	EMOVP(TRUE,E5.00,i_Analog_MAX_CH3);
	EMOVP(TRUE,E1.00,i_Analog_MIN_CH3);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH3);
	EMOVP(TRUE,E0.00,i_Digital_MIN_CH3);
	ELSIF Analog_Parameter_Setting_Fn_CH3 = H4 THEN
	EMOVP(TRUE,E20.00,i_Analog_MAX_CH3);
	EMOVP(TRUE,E0.00,i_Analog_MIN_CH3);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH3);
	EMOVP(TRUE,E0.00,i_Digital_MIN_CH3);
	ELSIF Analog_Parameter_Setting_Fn_CH3 = H5 THEN
	EMOVP(TRUE,E20.00,i_Analog_MAX_CH3);
	EMOVP(TRUE,E4.00,i_Analog_MIN_CH3);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH3;
	EMOVP(TRUE,E0.00,i_Digital_MIN_CH3);
END_IF;

IF Analog_Parameter_Setting_Fn_CH4 = H1 THEN
	EMOVP(TRUE,E10.00,i_Analog_MAX_CH4);
	EMOVP(TRUE,E-10.00,i_Analog_MIN_CH4);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH4);
	EMOVP(TRUE,E-16000.00,i_Digital_MIN_CH4);
	ELSIF Analog_Parameter_Setting_Fn_CH4 = H2 THEN
	EMOVP(TRUE,E5.00,i_Analog_MAX_CH4);
	EMOVP(TRUE,E0.00,i_Analog_MIN_CH4);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH4);
	EMOVP(TRUE,E0.00,i_Digital_MIN_CH4);
	ELSIF Analog_Parameter_Setting_Fn_CH4 = H3 THEN
	EMOVP(TRUE,E5.00,i_Analog_MAX_CH4);
	EMOVP(TRUE,E1.00,i_Analog_MIN_CH4);
	EMOVP(TRUE,E16000.00,i_Digital_MAX_CH4);
