(* Station Indexing *)
// Caution ! This confrigure need to set the A/D Remote module to Station 1
IF i_Box_Station_Selection = 1 THEN
	MOV(TRUE,H400,Staion_Index_For_Y_Set);
	MOV(TRUE,K100,Station_Index_For_D_Set);
	MOV(TRUE,Staion_Index_For_Y_Set,Staion_Index_For_Y_Set_Holding);
	MOV(TRUE,Station_Index_For_D_Set,Station_Index_For_D_Set_Holding);
	MOV(Enterling_Button,H000,Staion_Index_For_Y_Set);
	MOV(Enterling_Button,K0,Station_Index_For_D_Set);

	ELSIF i_Box_Station_Selection = 2 THEN
	MOV(TRUE,H500,Staion_Index_For_Y_Set);
	MOV(TRUE,K200,Station_Index_For_D_Set);
	MOV(TRUE,Staion_Index_For_Y_Set,Staion_Index_For_Y_Set_Holding);
	MOV(TRUE,Station_Index_For_D_Set,Station_Index_For_D_Set_Holding);
	MOV(Enterling_Button,H000,Staion_Index_For_Y_Set);
	MOV(Enterling_Button,K0,Station_Index_For_D_Set);
	
	ELSIF i_Box_Station_Selection = 3 THEN
	MOV(TRUE,H600,Staion_Index_For_Y_Set);
	MOV(TRUE,K300,Station_Index_For_D_Set);
	MOV(TRUE,Staion_Index_For_Y_Set,Staion_Index_For_Y_Set_Holding);
	MOV(TRUE,Station_Index_For_D_Set,Station_Index_For_D_Set_Holding);
	MOV(Enterling_Button,H000,Staion_Index_For_Y_Set);
	MOV(Enterling_Button,K0,Station_Index_For_D_Set);
	
	ELSIF i_Box_Station_Selection = 4 THEN
	MOV(TRUE,H700,Staion_Index_For_Y_Set);
	MOV(TRUE,K400,Station_Index_For_D_Set);
	MOV(TRUE,Staion_Index_For_Y_Set,Staion_Index_For_Y_Set_Holding);
	MOV(TRUE,Station_Index_For_D_Set,Station_Index_For_D_Set_Holding);
	MOV(Enterling_Button,H000,Staion_Index_For_Y_Set);
	MOV(Enterling_Button,K0,Station_Index_For_D_Set);
	
	ELSIF i_Box_Station_Selection = 5 THEN
	MOV(TRUE,H800,Staion_Index_For_Y_Set);
	MOV(TRUE,K500,Station_Index_For_D_Set);
	MOV(TRUE,Staion_Index_For_Y_Set,Staion_Index_For_Y_Set_Holding);
	MOV(TRUE,Station_Index_For_D_Set,Station_Index_For_D_Set_Holding);
	MOV(Enterling_Button,H000,Staion_Index_For_Y_Set);
	MOV(Enterling_Button,K0,Station_Index_For_D_Set);
	
	ELSIF i_Box_Station_Selection = 6 THEN
	MOV(TRUE,H900,Staion_Index_For_Y_Set);
	MOV(TRUE,K600,Station_Index_For_D_Set);
	MOV(TRUE,Staion_Index_For_Y_Set,Staion_Index_For_Y_Set_Holding);
	MOV(TRUE,Station_Index_For_D_Set,Station_Index_For_D_Set_Holding);
	MOV(Enterling_Button,H000,Staion_Index_For_Y_Set);
	MOV(Enterling_Button,K0,Station_Index_For_D_Set);
	
	ELSIF i_Box_Station_Selection = 10 THEN
	MOV(TRUE,H300,Staion_Index_For_Y_Set);
	MOV(TRUE,K700,Station_Index_For_D_Set);
	MOV(TRUE,Staion_Index_For_Y_Set,Staion_Index_For_Y_Set_Holding);
	MOV(TRUE,Station_Index_For_D_Set,Station_Index_For_D_Set_Holding);
	MOV(Enterling_Button,H000,Staion_Index_For_Y_Set);
	MOV(Enterling_Button,K0,Station_Index_For_D_Set);
	
	ELSE
	MOV(TRUE,H0,Staion_Index_For_Y_Set);
	MOV(TRUE,K0,Station_Index_For_D_Set);
END_IF;







;
